{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch gen-proto",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "proto-gen-types": "npx rimraf src/proto/generated && $(npm bin)/proto-loader-gen-types --longs=String --enums=String --keepCase=false --includeComments=true  --json=true --defaults=true --oneofs=true --grpcLib=@grpc/grpc-js --outDir=src/proto/generated src/proto/*.proto",
    "gen-proto": "bash scripts/generate_proto.sh"
  },
  "postinstall": "husky install",
  "dependencies": {
    "@flatten-js/interval-tree": "^1.1.2",
    "@grpc/grpc-js": "^1.8.21",
    "@grpc/proto-loader": "^0.7.8",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs-addons/in-memory-db": "^3.0.3",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "10.0.3",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/microservices": "10.0.3",
    "@nestjs/platform-express": "10.0.3",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.7",
    "@nestjs/typeorm": "^10.0.2",
    "@types/crypto-js": "^4.1.1",
    "axios": "^1.4.0",
    "bull": "^4.11.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "eslint-plugin-neverthrow": "^1.1.4",
    "husky": "^8.0.3",
    "ioredis": "^5.3.2",
    "mysql2": "^3.9.1",
    "neverthrow": "^6.0.0",
    "redis": "^3.1.2",
    "reflect-metadata": "0.1.13",
    "ts-proto": "^1.156.2",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "unidecode": "^0.1.8"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.5",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.3",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "20.3.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-unicorn": "^48.0.1",
    "jest": "29.6.1",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
